{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
    "api/v1/products.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
  }